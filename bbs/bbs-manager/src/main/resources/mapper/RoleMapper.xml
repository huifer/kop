<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.kop.bbs.repo.mapper.RoleMapper">
  <select id="byCode" resultType="com.github.kop.bbs.module.entity.Role">
    select *
    from bbs_role
    where 1 = 1
      and role_code = #{roleCode}
  </select>
</mapper>
